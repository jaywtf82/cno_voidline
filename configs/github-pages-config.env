
# C/No Voidline - GitHub Pages Configuration
# Generated: $(date)

NODE_ENV=production
VITE_DEPLOYMENT_TARGET=github-pages

# GitHub Pages specific
VITE_PUBLIC_PATH=/cno-voidline/
VITE_BASE_URL=https://yourusername.github.io/cno-voidline

# Storage Configuration (Frontend-only)
VITE_STORAGE_BACKEND=memory
VITE_REQUIRE_AUTH=false

# Application Settings
VITE_APP_NAME=cno-voidline
VITE_AI_PROVIDER=local
VITE_MAX_FILE_SIZE_MB=50
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_EXPORT=true
VITE_OPTIMIZE_BUNDLE=true
VITE_ENABLE_PWA=true
VITE_ENABLE_COMPRESSION=true
VITE_MINIFY=true

# GitHub Pages optimization
VITE_STATIC_DEPLOYMENT=true
VITE_CLIENT_ONLY=true
