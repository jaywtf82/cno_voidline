
# C/No Voidline - Replit Deployment Configuration
# Generated: $(date)

NODE_ENV=production
PORT=5000
HOST=0.0.0.0

# Replit-specific settings
REPLIT_DEPLOYMENT=true
VITE_DEPLOYMENT_TARGET=replit

# Storage Configuration
VITE_STORAGE_BACKEND=memory
VITE_REQUIRE_AUTH=false

# Application Settings
VITE_APP_NAME=cno-voidline
VITE_AI_PROVIDER=local
VITE_MAX_FILE_SIZE_MB=100
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_EXPORT=true
VITE_OPTIMIZE_BUNDLE=true
VITE_ENABLE_PWA=true
VITE_ENABLE_COMPRESSION=true

# Security Settings
SESSION_TIMEOUT_MINUTES=60
CORS_ORIGINS=*
ENABLE_RATE_LIMITING=false
ENABLE_HTTPS=true

# Replit Database (if using Replit DB)
REPLIT_DB_URL=${REPLIT_DB_URL}

# Production Optimization
VITE_MINIFY=true
DEBUG=false
LOG_LEVEL=info
