
{
  "$schema": "https://replit.com/public/replit-deploy-schema.json",
  "name": "cno-voidline",
  "build": {
    "main": "server/index.ts",
    "run": "npm run start"
  },
  "deployment": {
    "type": "autoscale",
    "maxInstances": 5,
    "minInstances": 0,
    "machineType": "shared-cpu-1x",
    "healthcheck": "/health",
    "ports": [
      {
        "name": "web",
        "value": 5000,
        "protocol": "http"
      }
    ]
  },
  "env": {
    "NODE_ENV": "production",
    "VITE_DEPLOYMENT_TARGET": "replit",
    "VITE_STORAGE_BACKEND": "memory",
    "VITE_AI_PROVIDER": "local"
  }
}
