
# C/No Voidline - Hugging Face Spaces Configuration
# Generated: $(date)

NODE_ENV=production
VITE_DEPLOYMENT_TARGET=huggingface

# Hugging Face specific
HUGGINGFACE_SPACES=true
VITE_BASE_URL=https://huggingface.co/spaces/yourusername/cno-voidline

# Storage Configuration (Frontend-only for Spaces)
VITE_STORAGE_BACKEND=memory
VITE_REQUIRE_AUTH=false

# Application Settings
VITE_APP_NAME=cno-voidline
VITE_AI_PROVIDER=local
VITE_MAX_FILE_SIZE_MB=50
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_EXPORT=true
VITE_OPTIMIZE_BUNDLE=true
VITE_ENABLE_PWA=false
VITE_ENABLE_COMPRESSION=true
VITE_MINIFY=true

# Hugging Face optimization
VITE_STATIC_DEPLOYMENT=true
VITE_CLIENT_ONLY=true
VITE_DEMO_MODE=true
