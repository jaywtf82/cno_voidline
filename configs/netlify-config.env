
# C/No Voidline - Netlify Configuration
# Generated: $(date)

NODE_ENV=production
VITE_DEPLOYMENT_TARGET=netlify

# Netlify specific
NETLIFY_FUNCTIONS_ENABLED=true
VITE_BASE_URL=https://cno-voidline.netlify.app

# Storage Configuration
VITE_STORAGE_BACKEND=memory
VITE_REQUIRE_AUTH=false

# Application Settings
VITE_APP_NAME=cno-voidline
VITE_AI_PROVIDER=local
VITE_MAX_FILE_SIZE_MB=100
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_EXPORT=true
VITE_OPTIMIZE_BUNDLE=true
VITE_ENABLE_PWA=true
VITE_ENABLE_COMPRESSION=true
VITE_MINIFY=true

# Security Settings
SESSION_TIMEOUT_MINUTES=60
CORS_ORIGINS=https://cno-voidline.netlify.app
ENABLE_RATE_LIMITING=true
ENABLE_HTTPS=true

# Netlify optimization
VITE_EDGE_FUNCTIONS=true
VITE_FORM_HANDLING=true
