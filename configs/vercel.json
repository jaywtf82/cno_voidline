
{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "cno-voidline",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "buildCommand": "cp configs/vercel-config.env .env.production && npm run build:vercel",
  "outputDirectory": "dist",
  "installCommand": "npm ci",
  "devCommand": "npm run dev",
  "framework": "vite",
  "routes": [
    {
      "src": "/config.html",
      "dest": "/config.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VITE_DEPLOYMENT_TARGET": "vercel"
  }
}
