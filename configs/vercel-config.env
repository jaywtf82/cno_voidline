
# C/No Voidline - Vercel Configuration
# Generated: $(date)

NODE_ENV=production
VITE_DEPLOYMENT_TARGET=vercel

# Vercel specific
VERCEL_FUNCTIONS_ENABLED=true
VITE_BASE_URL=https://cno-voidline.vercel.app

# Storage Configuration
VITE_STORAGE_BACKEND=memory
VITE_REQUIRE_AUTH=false

# Application Settings
VITE_APP_NAME=cno-voidline
VITE_AI_PROVIDER=local
VITE_MAX_FILE_SIZE_MB=100
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_EXPORT=true
VITE_OPTIMIZE_BUNDLE=true
VITE_ENABLE_PWA=true
VITE_ENABLE_COMPRESSION=true
VITE_MINIFY=true

# Security Settings
SESSION_TIMEOUT_MINUTES=60
CORS_ORIGINS=https://cno-voidline.vercel.app
ENABLE_RATE_LIMITING=true
ENABLE_HTTPS=true

# Vercel optimization
VERCEL_ANALYTICS=true
VERCEL_SPEED_INSIGHTS=true
